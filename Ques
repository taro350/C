#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    string A, B, C;
    
    cout << "Enter a noun:";
    cin >> A;

    cout << "Enter adjective:";
    cin >> B;

    cout << "Enter name:";
    cin >> C;


    cout <<"You like "<<A << endl;
    cout <<"I like to be " << endl;
    cout <<"You love "<<C<< endl;



    return 0;
}
